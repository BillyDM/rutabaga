#!/usr/bin/env python

top = '..'

def build(bld):
    bld.program(
        source='test.c',
        use=['RUTABAGA'],
        target='test')

    bld.program(
        source='txtest.c',
        use=['RUTABAGA'],
        target='txtest')

    if bld.env.LIB_JACK:
        bld.program(
            source='cabbage_patch.c',
            use=[
                'RUTABAGA',
                'JACK'],

            target='cabbage_patch')
